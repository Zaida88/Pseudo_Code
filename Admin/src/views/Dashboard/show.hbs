        <div id="data">
            <img src="/img/show/icon-test.png" height="85" width="85"><br>
            <label><h6><b>Pruebas</b></h6></label>
            <h5>{{test}}</h5>
        </div>
        <div id="data">
            <img src="/img/show/icon-exercise.png" height="85" width="85"><br>
            <label><h6><b>Ejercicios</b></h6></label>
            <h5>{{exercise}}</h5>
        </div>
        <div id="data">
            <img src="/img/show/icon-users.png" height="85" width="85"><br>
            <label><h6><b>Usuarios</b></h6></label>
            <h5>{{user}}</h5>
