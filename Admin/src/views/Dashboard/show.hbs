            <img src="/img/show/icon-project.png" height="85" width="85"><br>
            <label><h6><b>Proyectos</b></h6></label>
            <h5>{{project}}</h5>
        </div>
        <div id="data">
            <img src="/img/show/icon-lenguaje.png" height="85" width="85"><br>
            <label><h6><b>Lenguajes</b></h6></label>
            <h5>{{language}}</h5>
