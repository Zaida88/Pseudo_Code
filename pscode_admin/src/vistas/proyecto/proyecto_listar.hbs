<link rel="stylesheet" href="/CSS/proyecto.css" />

<form action="/proyecto/listar/{{user.idusuario}}" method="get"></form>
<div id="contenedor">
  {{#each lista}}
    <div id="pscode">
      <label class="titulo">Proyectos</label>
      <h1 class="titulo">{{nombre_pro}}</h1>
    </div>
    <div id="descripcion">
      <label class="label">Descripcion</label>
      <h1 class="titulo">{{descripcion}}</h1>
    </div>
    <div id="mision">
      <label class="label">Mision</label>
      <h1 class="titulo">{{mision}}</h1>
    </div>
    <div id="vision">
      <label class="label">Vision</label>
      <h1 class="titulo">{{vision}}</h1>
    </div class="label">
    <div id="editar">
      <a href="/proyecto/editar/{{iddatosproyecto}}"><button class="boton">Editar</button></a>
    </div>
  {{/each}}
  </div>
  {{#each listaObjetivos}}
    <div id="objetivos">
      <label class="label">Objetivos:</label>
      <h1 class="titulo">{{objetivos}}</h1>
    </div>
  {{/each}}
  <div id="clasificacion">
    <a href="/clasificacion/listar/{{user.idusuario}}"><button class="boton boton--mitad-oculto"
      >Clasificacion</button></a>
  </div>
</div>